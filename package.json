{
    "name": "frontend",
    "version": "0.0.0+sha.03640597",
    "scripts": {
        "start": "vite --host",
        "build": "vite build",
        "watch": "nodemon --watch ../../ -e go,ts,vue --signal SIGHUP --exec 'npm run build && go run ../../main.go ui'",
        "static-check": "vue-tsc --noEmit",
        "bump": "semverity patch",
        "prepare": "husky install"
    },
    "dependencies": {
        "@vueuse/core": "^8.7.5",
        "floating-vue": "^2.0.0-beta.17",
        "highlight.js": "^11.5.1",
        "monaco-editor": "^0.33.0",
        "normalize.css": "^8.0.1",
        "pinia": "^2.0.14",
        "vue": "^3.2.37"
    },
    "devDependencies": {
        "@iconify/json": "^2.1.65",
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/eslint-config-typescript": "^11.0.0",
        "autoprefixer": "^10.4.7",
        "eslint": "^8.18.0",
        "eslint-plugin-vue": "^9.1.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.2",
        "nodemon": "^2.0.18",
        "postcss": "^8.4.14",
        "prettier": "^2.7.1",
        "sass": "^1.53.0",
        "semverity": "^0.3.0",
        "tailwindcss": "^3.1.4",
        "typescript": "^4.7.4",
        "unplugin-icons": "^0.14.5",
        "unplugin-vue-components": "^0.19.9",
        "vite": "^2.9.12",
        "vite-plugin-fonts": "^0.6.0",
        "vue-tsc": "^0.38.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,vue}": "eslint --fix",
        "*": "prettier -w -u"
    }
}
